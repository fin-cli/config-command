parameters:
  level: 9
  paths:
    - src
    - config-command.php
  scanDirectories:
    - vendor/fin-cli/fin-cli/php
  scanFiles:
    - vendor/php-stubs/finpress-stubs/finpress-stubs.php
  treatPhpDocTypesAsCertain: false
  dynamicConstantNames:
  	- FIN_DEBUG
  	- FIN_DEBUG_LOG
  	- FIN_DEBUG_DISPLAY
  ignoreErrors:
    - identifier: missingType.iterableValue
    - identifier: missingType.parameter
    - identifier: missingType.return
